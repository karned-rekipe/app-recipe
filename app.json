{
  "expo": {
    "name": "Rekipe",
    "slug": "app-recipe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "rekipe",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "bzh.koden.rekipe"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "manifest": {
        "name": "Rekipe - Application de Recettes",
        "short_name": "Rekipe",
        "description": "Application de recettes simple et moderne pour découvrir et gérer vos recettes préférées.",
        "theme_color": "#ffb300",
        "background_color": "#fffbe6",
        "display": "standalone",
        "orientation": "portrait",
        "lang": "fr",
        "scope": "/",
        "start_url": "/"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7207ced0-6749-4f56-83a1-5e0ee18ac4d4"
      }
    },
    "owner": "skit"
  }
}
